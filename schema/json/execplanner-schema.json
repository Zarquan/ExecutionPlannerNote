{
  "$id": "https://bit.ly/ivoa-execplanner",
  "$schema": "https://json-schema.org/draft-07/schema",
  "definitions": {
    "ExecutableLeaseType": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://bit.ly/ivoa-executable#/$defs/ExecutableTaskType"
        }
      ],
      "properties": {
        "lease": {
          "startdate": {
            "$ref": "https://bit.ly/ivoa-executable#/$defs/ISO8601DateType"
            },
          "enddate": {
            "$ref": "https://bit.ly/ivoa-executable#/$defs/ISO8601DateType"
            }
          }
        }
      },
    "ExecutableOfferType": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://bit.ly/ivoa-executable#/$defs/ExecutableLeaseType"
        }
      ],
      "properties": {
        "request": "https://bit.ly/ivoa-executable#/$defs/UUIDType",
        "offer": {
          "startdate": {
            "$ref": "https://bit.ly/ivoa-executable#/$defs/ISO8601DateType"
            },
          "enddate": {
            "$ref": "https://bit.ly/ivoa-executable#/$defs/ISO8601DateType"
          }
        }
      },

    "ExecutableRejectType": {
      "type": "object",
      "allOf": [
        {
          "$ref": "https://bit.ly/ivoa-executable#/$defs/ExecutableLeaseType"
        }
      ],
      "properties": {
        "reason": "string",
        "message": "string"
        }
      },

    "ExecutableRequestType": {
      "type": "object",
      "properties": {
        "requests": {
          "type": "array",
          "$ref": "#/$defs/ExecutableLeaseType"
          }
        }
      },
    "ExecutableResponseType": {
      "type": "object",
      "properties": {
        "offered": {
          "type": "array",
          "$ref": "#/$defs/ExecutableOfferType"
          },
        "rejected": {
          "type": "array",
          "$ref": "#/$defs/ExecutableRejectType"
          }
        }
      }
    }
}
}
