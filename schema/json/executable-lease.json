{
    "$id": "https://bit.ly/ivoa-executable-lease",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "allOf": [
        {"$ref": "https://bit.ly/ivoa-executable-task"}
    ],
    "properties": {
        "status": {
            "enum": [
                "REQUESTED",
                "OFFERED",
                "ACCEPTED",
                "REJECTED",
                "EXPIRED"
            ]
        },
        "messages": {
            "type": "array",
            "items": {"type": "string"}
        },
        "lease": {
            "type": "object",
            "properties": {
                "startdate": {"$ref": "https://bit.ly/ivoa-executable-task#/definitions/ISO8601DateType"},
                "enddate": {"$ref": "https://bit.ly/ivoa-executable-task#/definitions/ISO8601DateType"}
            }
        },
        "offer": {
            "type": "object",
            "properties": {
                "startdate": {"$ref": "https://bit.ly/ivoa-executable-task#/definitions/ISO8601DateType"},
                "enddate": {"$ref": "https://bit.ly/ivoa-executable-task#/definitions/ISO8601DateType"}
            }
        }
    }
}